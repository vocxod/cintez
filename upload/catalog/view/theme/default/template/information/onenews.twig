{{ header }}

<div id="information-newslist" class="container">
  <ul class="breadcrumb">
    {% for breadcrumb in breadcrumbs %}
    <li>
      {% if loop.index != 1 %}
      &#x27f6;
      {% endif %}
      <a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
    {% endfor %}
  </ul>

  <div class="row">
    <div class="col-sm-12 ">
      
        <div class="col-sm-8 article_start">
            {#<img src="{{ is_new.image }}" alt="">#}
            <br>
            {{is_new.image}}
            <h4 style="font-weight:bold;">{{ is_new.title }}</h4>
            <p>
              {{ is_new.description }}
            </p>
        </div>
       
        <div class="col-sm-4 ">
          
          <h3 class="h2"><span class="green-uline">{{ heading_title_pub }}</span></h3>

