    if( dataid == 168 ) $('#menu_' + dataid).css('background-position', '-252px -30px')
    if( dataid == 113 ) $('#menu_' + dataid).css('background-position', '-283px -30px') 
    $('#panel_link_' + dataid).css('color', '#111');
