    if( dataid == 10 )  $('#menu_' + dataid).css('background-position', '-93px  0px')
    if( dataid == 131 ) $('#menu_' + dataid).css('background-position', '-125px  0px') 
    if( dataid == 72 )  $('#menu_' + dataid).css('background-position', '-157px 0px')
    if( dataid == 117 ) $('#menu_' + dataid).css('background-position', '-190px 0px')
    if( dataid == 150 ) $('#menu_' + dataid).css('background-position', '-220px 0px') 
    if( dataid == 168 ) $('#menu_' + dataid).css('background-position', '-252px 0px')
    if( dataid == 113 ) $('#menu_' + dataid).css('background-position', '-283px 0px')          
    $('#panel_link_' + dataid).css('color', 'white');
  }) 
  $('.panel-menu-item').on( "mouseout", function(){
    $(this).css('background', '#FFFFFF')
    $(this).css('color', '#111')
    
    if( dataid == 94 )  $('#menu_' + dataid).css('background-position', '0px    -30px')
    if( dataid == 87 )  $('#menu_' + dataid).css('background-position', '-30px  -30px')
    if( dataid == 54 )  $('#menu_' + dataid).css('background-position', '-61px  -30px')
